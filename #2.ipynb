   "source": [
    "prev = 0\n",
    "sig = 1\n",
    "temp_prev = 0\n",
    "\n",
    "fibonacci_pares = Set{Int}()\n",
    "\n",
    "while prev + sig < 4000000\n",
    "    if iseven(prev + sig)\n",
    "        push!(fibonacci_pares, prev + sig)\n",
    "    end\n",
    "\n",
    "    temp_prev = prev\n",
    "    prev = sig\n",
    "    sig = sig + temp_prev\n",
    "    \n",
    "end\n",
    "\n",
    "print(fibonacci_pares)\n",
    "\n",
    "sum(fibonacci_pares)\n"
   ]
  }
