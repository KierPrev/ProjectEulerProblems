   "source": [
    "function quienEs(n)\n",
    "    for a in 1:n\n",
    "        for b in 2:n\n",
    "            if a + b + sqrt(a^2 + b^2) == n\n",
    "                c = n-a-b\n",
    "                print(a, b, c)\n",
    "                return a * b * c\n",
    "            end\n",
    "        end\n",
    "    end\n",
    "end"
   ]
