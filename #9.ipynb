   "source": [
    "function quienEs(n)\n",
    "    for a in 1:n\n",
    "        for b in 2:n\n",
    "            if a + b + sqrt(a^2 + b^2) == n\n",
    "                return a, b, n-a-b\n",
    "            end\n",
    "        end\n",
    "    end\n",
    "    return\n",
    "end"
   ]
